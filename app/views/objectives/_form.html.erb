<!DOCTYPE html>
<html>
<head>
  <title></title>
</head>
<body>
  <section class="content invoice">
    <%= form_for(objective) do |f| %>
  <% if objective.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(objective.errors.count, "error") %> prohibido guardar este Objetivo:</h2>

      <ul>
      <% objective.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>


  <div class="row">
  <div class="form-group">
  <div class="field">
    <%= f.label :texto %>
  </div>
  <div class="field">
    <%= f.text_area :texto %>
  </div>
  </div>

  <div class="form-group">
  <div class="field">
    <%= f.label :zone_id, "Area " %>
     <%= collection_select( :objective, :zone_id, Zone.all, :id, :nombre, { :multiple => false }, { :class => "my-custom-class" } ) %>
  </div>
  </div>

  <div class="form-group">
  <div class="field">
    <%= f.label :classification_id, "Clasificacion " %>
    <%= collection_select( :objective, :classification_id, Classification.all, :id, :nombre, { :multiple => false }, { :class => "my-custom-class" } ) %>
  </div>
  </div>

  <div class="form-group">
  <div class="actions">
    <%= f.submit %>
  </div>
  </div>
<% end %>
</div>
  </section>
</body>
</html>


